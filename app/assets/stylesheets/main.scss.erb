/*======================================= top navigation =======================================*/
.menu{
  background: rgba( 255, 255, 255, 1);
  //border-bottom: none;
  //box-shadow: 0px 15px 0px -14px #536132;
  color: #536132 !important;  
}

.navbar-shrink{
  background: rgba( 255, 255, 255, 0.7);
  box-shadow: none;
}

.navbar .navbar-collapse {
}
.no-touch .navbar-nav a:hover{
