}
.cart_label_container{
  padding-bottom: 10px; 
  text-align: right;  
}

@media (max-width: 1065px) {
  .cart-dropdown-menu{
    width: auto !important;
  }
  .cart_lists li{
    text-align: center;
  }
  .cart_lists li a{
    text-align:left;
  }  
