}
.no-background {
    font: 20px sans-serif;
    margin-top: 25px;
    margin-bottom: 25px;    
    text-align: center;  
    position: relative;
    overflow: hidden;
    span {
        display: inline-block;
        vertical-align: baseline;
        zoom: 1;
        *display: inline;
        *vertical-align: auto;
        position: relative;
        padding: 0 20px;
        &:before, &:after {
            content: '';
            display: block;
            width: 1000px;
            position: absolute;
            top: 0.73em;
            border-top: 1px solid #536132;
        }
        &:before { right: 100%; }
        &:after { left: 100%; }
    }
}
