/*======================================= top navigation =======================================*/
.menu{
  background: rgba( 255, 255, 255, 1);
  border-bottom: none;
  box-shadow: 0px 15px 0px -14px #536132;
  color: #f1f1f1 !important;  
}

.navbar-shrink{
  background: rgba( 255, 255, 255, 0.8);
  box-shadow: none;
}

.navbar .navbar-collapse {
}
.navbar-nav a:hover{
  opacity: 0.8;  
}
input.rounded {
    border: 1px solid #ccc;
    -moz-border-radius: 17px;
    -webkit-border-radius: 17px;
    border-radius: 17px; 
    padding: 4px 7px;
    outline: 0;
    -webkit-appearance: none;
    background: rgba( 255, 255, 255, 1); 
    border: none;     
}
input.rounded:focus {
    border: none; 
}
.navbar-right>li{
  padding-right: 10px;
}
.navbar-right>li>a.menu_right {
    padding: 7px 7px;
}
.navbar-right>li>a.menu_right:focus, .navbar-right>li>a.menu_right:active{
  background: none !important;
}
.navbar-right>li>a.menu_right img{
  height: 38px;
  width: 38px;
  vertical-align: middle;
}
.navbar-brand{
}
.navbar-brand img{
  height: 30px; 
  margin-top: -5px;
}
.mobile_menu_label{
  visibility: hidden;  
  display: block;
  position: absolute; 
  font-size: 20px; 
  top: 16px;
  left: 47px;  
}
.menu_coupons {
  display: block;
  text-align: center;
  top: 18px;
  position: absolute;
  line-height: 20px;
  font-size: 18px;
  left: 3px;
  width: 45px;
  color: #b5841b;
}
.badge {
  display: block;
  position: absolute;
  top: 2px;
  right: 1px;
  line-height: 20px;
  height: 20px;
  padding: 0 7px;
}
.badge-danger {
  background-color: #d43f3a;
}
.badge-toggle {
  display: block;
  position: absolute;
  top: -6px;
  right: -10px;
  line-height: 20px;
  height: 20px;
  padding: 0 7px;
}
.dropdown-menu{
  margin-top: 10px !important;
  color: #605856;  
  background: #ffffff;    
  box-shadow: none;  
  border-radius: 10px !important;
}
.dropdown-menu:before {
  position: absolute;
  top: -17px;
  right: 20px;
  display: inline-block;
  border-right: 17px solid transparent;
  border-bottom: 17px solid #ccc;
  border-left: 17px solid transparent;
  border-bottom-color: rgba(0, 0, 0, 0.2);
  content: '';
}

.dropdown-menu:after {
  position: absolute;
  top: -18px;
  right: 15px;
  display: inline-block;
  border-right: 18px solid transparent;
  border-bottom: 18px solid #f3f3f2;
  border-left: 18px solid transparent;
  content: '';
}
@media (max-width: 1065px) {
  .menu{
    background: #ffffff;
    border-bottom: none;
    box-shadow: 0px 1px 0px 0px #E7E7E7;  
    color: #f1f1f1 !important;    
  }
  .dropdown-menu:before{
    border: none;
  }  
  .dropdown-menu:after{
    border: none;
  }
  .mobile_menu_label{
    visibility: visible;    
  }
}
