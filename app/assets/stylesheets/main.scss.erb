}
.navbar-form{
  border-top: none;
}
input.rounded {
      border-radius: 17px; 
      box-shadow: none !important;  
}
input.rounded:focus {
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;  
    border-color: #4fb040;
}
.menu_search_button{
  background: none;
  border: none;
}
.menu_search_button:focus, .menu_search_button:active{
  border: 0 !important;
  box-shadow: none !important;
  background: none !important;
  outline: 0 !important;
}
.no-touch .menu_search_button:hover{
  background: none;
}
.menu_search_button img{
  height: 16px;
}
.navbar-right>li{
  padding-right: 10px;
}
.navbar-right>li>a.menu_right {
  padding: 7px 7px;
  color: #536132 !important; 
}
.navbar-right>li>a.menu_right:focus, .navbar-right>li>a.menu_right:active{
  background: none !important;
}
.navbar-right>li>a.menu_right img{
  height: 38px;
  width: 38px;
  vertical-align: middle;
}
.navbar-brand{
}
.navbar-brand img{
  height: 30px; 
  margin-top: -5px;
}
.mobile_menu_label{
  visibility: hidden;  
  display: block;
  position: absolute; 
  font-size: 20px; 
  top: 16px;
  left: 47px;  
}
.menu_coupons {
  display: block;
  text-align: center;
  top: 18px;
  position: absolute;
  line-height: 20px;
  font-size: 18px;
  left: 3px;
  width: 45px;
  color: #b5841b;
}
.badge {
  display: block;
  position: absolute;
  top: 2px;
  right: 1px;
  line-height: 20px;
  height: 20px;
  padding: 0 7px;
}
.badge-danger {
  background-color: #d43f3a;
}
.badge-toggle {
  display: block;
  position: absolute;
  top: -6px;
  right: -10px;
  line-height: 20px;
  height: 20px;
  padding: 0 7px;
}
.dropdown-menu{
  margin-top: 10px !important;
  color: #433E3C;  
  background: #ffffff;    
  box-shadow: none;  
  border-radius: 10px !important;
}
.dropdown-menu:before {
  position: absolute;
  top: -17px;
  right: 20px;
  display: inline-block;
  border-right: 17px solid transparent;
  border-bottom: 17px solid #ccc;
  border-left: 17px solid transparent;
  border-bottom-color: rgba(0, 0, 0, 0.2);
  content: '';
}

.dropdown-menu:after {
  position: absolute;
  top: -18px;
  right: 15px;
  display: inline-block;
  border-right: 18px solid transparent;
  border-bottom: 18px solid #ffffff;
  border-left: 18px solid transparent;
  content: '';
}
@media (max-width: 1094px) {
  .menu{
    background: #ffffff;
    border-bottom: none;
    box-shadow: 0px 1px 0px 0px #E7E7E7;  
    color: #f1f1f1 !important;    
  }
  .dropdown-menu:before{
    border: none;
  }  
  .dropdown-menu:after{
    border: none;
  }
  .mobile_menu_label{
    visibility: visible;    
  }
}
