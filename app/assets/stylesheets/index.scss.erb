.service_intro_step_container {
    padding: 30px 0px 30px 30px;
    margin-top: 5px;    
    margin-right: 5px;
    background: #efefef;
    position: relative;
    display: inline-block;
	font-size: 17px;	    
}
.service_intro_step_container:before {
    width: 0;
    height: 0;
    border-top: 70px inset transparent;
    border-bottom: 70px inset transparent;
    border-left: 20px solid #fff;
    position: absolute;
    content: "";
    top: 0;
    left: 0;
}
.service_intro_step_container:after {
    width: 0;
    height: 0;
    border-top: 70px inset transparent;
    border-bottom: 70px inset transparent;
    border-left: 20px solid #efefef;
    position: absolute;
    content: "";
    top: 0;
    right: -20px;
    z-index: 2;
}
.service_intro_step_title{
	color: rgb(234, 90, 26);
}
.service_intro_step_content{
	width: 280px;
	font-size: 16px;
