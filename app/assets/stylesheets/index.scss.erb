header{
    //background-image: url(<%=asset_path 'index/default_index.jpg'%>);
    //background-attachment: scroll;
    //background-position: center center;   
    //background-size: cover;
    text-align: center;
    color: #FFF;
}
header .intro-text{
    margin-top: 30vh;
}
header .intro-text .intro-lead-in{
    font-style:italic;
    font-size:40px;
    line-height:40px;
    margin-bottom:25px;
}
header .intro-text .intro-heading{
    text-transform:uppercase;
    font-weight:700;
    font-size: 60px;
    line-height:60px;
    margin-bottom: 70px;
}
.intro-nav{
}
.intro-nav-button_1{
    color: #ffffff;
    font-size: 20px;
    line-height: 20px;
    letter-spacing: 3px;    
    padding: 15px 60px;      
    background: rgba(234, 89, 24, 0.75);
    border-radius: 0px;
} 
.intro-nav-button_2{
    color: #ffffff;
    font-size: 20px;
    line-height: 20px;
    letter-spacing: 3px;    
    padding: 15px 60px;      
    background: rgba(88, 210, 62, 0.75);
    border-radius: 0px;
} 
.no-touch .intro-nav-button_1:hover, .intro-nav-button_1:focus{
    text-decoration: none;
    color: #ffffff;
    background: rgba(234, 89, 24, 1);
}  
.no-touch .intro-nav-button_2:hover, .intro-nav-button_2:focus{
    text-decoration: none;
    color: #ffffff;
    background: rgba(88, 210, 62, 1);
}  
 
@media (max-width:768px){
  header .intro-text{
    margin-top: 40%;
  } 
  header .intro-text .intro-lead-in{
    font-style:italic;
    font-size: 27px;
    line-height: 27px;
    margin-bottom: 27px
  } 
  header .intro-text .intro-heading{
    text-transform:uppercase;
    font-weight:700;
    font-size: 40px;
    line-height:40px;
    margin-bottom:25px
  }  
  .intro-nav-button_1, .intro-nav-button_2{
      font-size: 16px;
      line-height: 60px;
      letter-spacing: 2px;    
