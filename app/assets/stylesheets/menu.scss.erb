/*======================================= top navigation =======================================*/
.menu{
  background: rgba( 255, 255, 255, 1);
  color: #433E3C !important;  
}
.navbar-brand{
  margin-top: 15px;  
  height: 60px;   
}
.navbar-brand img{
  height: 30px; 
}
.text-nav li a{
  text-align: center;  
  color: #777 !important; 
}
.no-touch .text-nav li a:hover{
  background: #EFEFEF !important;   
}
.text-nav .text-nav_img{
  width: 45px;
}
.text-nav .text-nav_text{
  margin-top: 10px;
  font-size: 16px;  
}
.navbar-form{
  margin-top: 30px;
}
.navbar-form input{
  padding: 0px 10px;
}

input.rounded {
  border-radius: 17px; 
  box-shadow: none !important;  
}
input.rounded:focus {
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;  
  border-color: #ff5600;
}
.menu_search_button{
  background: none;
  border: none;
}
.menu_search_button:focus, 
.menu_search_button:active{
  border: 0 !important;
  box-shadow: none !important;
  background: none !important;
  outline: 0 !important;
}
.no-touch .menu_search_button:hover{
  background: none;
}
.menu_search_button img{
  height: 16px;
}
.navbar-right{
  margin-top: 20px;
}
.navbar-right>li .menu_right {
  position: relative;
  line-height: 20px;  
  display: block;
  padding: 12px 12px;
  color: #433E3C !important; 
}
.no-touch .navbar-right>li:hover{
  background: #EFEFEF !important; 
}
.navbar-right>li .menu_right:focus, 
.navbar-right>li .menu_right:active{
  background: none !important; 
} 
.navbar-right>li .menu_right img{
  height: 28px;
  width: 28px;
  vertical-align: middle;
}
.badge {
  display: block;
  position: absolute;
  top: 6px;
  right: 2px;
  font-size: 10px !important;
  line-height: 18px;
  height: 18px;
  padding: 0 6px;
}
.badge-orange {
  background-color: #ff5600;
}
.badge-toggle {
  display: block;
  position: absolute;
  top: -6px;
  right: -10px;
  line-height: 20px;
  height: 20px;
  padding: 0 7px;
}
.dropdown-menu{
  color: #433E3C;  
  background: #ffffff;    
  box-shadow: none;  
  border-radius: 10px !important;  
}
//nav-shrink
.navbar-shrink .navbar-brand{
    -webkit-transition: all .3s;
    -moz-transition: all .3s;
    transition: all .3s;  
    margin-top: 0px;  
    height: 52px;   
  }  
.navbar-shrink .navbar-brand img{
    height: 22px; 
  } 
.navbar-shrink .text-nav .text-nav_img{  
    width: 22px;
    vertical-align: middle;   
    display: inline-block; 
  }
.navbar-shrink .text-nav .text-nav_text{
    line-height: 15px;  
    font-size: 15px;
    margin-top: 0px;
    vertical-align: middle;    
    display: inline-block; 
  }     
.navbar-shrink .navbar-form{
    margin: 8px 0px;
    margin-top: 10px;
  }  
.navbar-shrink .navbar-right{
    margin-top: 0px;
}  
//responsive
@media (min-width: 1195px) {
  .no_collapse_point{
    display: none !important;
  }  
}
@media (max-width: 1195px) {

  .collapse_point{
    display: none !important;
  }
  .navbar-shrink .navbar-brand,
  .navbar-brand{
    margin-top: 0px;  
    height: 50px;   
  }  
  .navbar-brand img{
    height: 24px; 
  } 
  .text-nav li a{
    text-align: left;      
  }
  .navbar-shrink .text-nav .text-nav_img, 
  .text-nav .text-nav_img{
    width: 30px;
    display: inline-block;
  }
  .navbar-shrink .text-nav .text-nav_text, 
  .text-nav .text-nav_text{
    font-size: 18px;    
    height: 25px;
    line-height: 25px;
    margin-top: 0px;
    margin-left: 10px;
    display: inline-block;
    vertical-align: middle;
  }
  .navbar-shrink .navbar-form,     
  .navbar-form{
    border: none;
    margin: 0px;
  }  
  input.rounded {
    font-size: 14px;
    width: auto !important;     
  }  
  .navbar-right{
    margin-top: 0px;
  }  
  .mobile_menu_label{
    display: block;
    position: absolute; 
    font-size: 17px; 
    top: 17px;
    left: 60px;  
    color: #A7A7A7 !important;     
  }  
  .menu_right>.badge{
    position: absolute;
    top: 2px;
    left: 32px;
    right: auto;
  }
  .navbar-toggle{
    margin-top: 9px;
    margin-bottom: 9px; 
  } 
}
@media (min-width: 380px) {
  .logo-brand{
    display: none !important;
  }
}
@media (max-width: 380px) {
  .smallfarmer01-brand{
    display: none !important;
  }
}
















