/*======================================= manage =======================================*/
.manage_lists{
  text-align: center;
  padding: 13px 0px;
  min-width: 110px;   
}
.manage_lists li{
}
.manage_lists li a{
  font-size: 15px;
  color: #433E3C;  
  padding: 3px 0px !important;  
}
@media (max-width: 960px) {

}
/*======================================= checkout =======================================*/
.cart-dropdown-menu {
  visibility: visible;
  padding: 20px 20px;  
  width: 250px !important;
}
.cart_lists li{
  border-bottom: 1px solid #E7E7E7;   
  padding-bottom: 10px;  
  margin-bottom: 10px;
}
.cart_lists li a{
  color: #433E3C;  
}
.no-touch .cart_lists li a:hover{
  opacity: 0.8;
}
.cart_image{
  height: 70px; 
  width: 70px;
  display: inline-block;
  vertical-align: middle;
}
.cart_content{
  width: 100px;
  margin-left: 15px;
  display: inline-block; 
  vertical-align: middle;   
}
.cart_op{
  margin-left: 12px;
  display: inline-block; 
  vertical-align: middle;   
}
.cart_label_container{
  padding-bottom: 10px; 
  text-align: right;  
}

@media (max-width: 1065px) {
  .cart-dropdown-menu{
    width: auto !important;
  }
  .cart_lists li{
    text-align: center;
  }
  .cart_lists li a{
    text-align:left;
  }  
  .cart_label_container{
    visibility: hidden;
  } 
}