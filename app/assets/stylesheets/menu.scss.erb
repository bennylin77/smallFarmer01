/*======================================= top navigation =======================================*/
.menu{
  background: rgba( 255, 255, 255, 1);
  color: #433E3C !important;  
}
.navbar-brand{
  margin-top: 15px;  
  height: 60px;   
}
.navbar-brand img{
  height: 30px; 
}
//.no-touch .navbar-nav a:hover{
//  color: #757575 !important; 
//}
.text-nav li a{
  font-size: 16px;
  color: #A7A7A7 !important; 
}
.no-touch .text-nav li a:hover{
  background: #EFEFEF !important;   
}
.text-nav .text-nav_img{
  width: 45px;
}
.text-nav .text-nav_text{
  margin-top: 10px;
}
.navbar-form{
  margin-top: 30px;
}
input.rounded {
  //border-radius: 17px; 
  box-shadow: none !important;  
}
input.rounded:focus {
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;  
  border-color: #ff5600;
}
.menu_search_button{
  background: none;
  border: none;
}
.menu_search_button:focus, 
.menu_search_button:active{
  border: 0 !important;
  box-shadow: none !important;
  background: none !important;
  outline: 0 !important;
}
.no-touch .menu_search_button:hover{
  background: none;
}
.menu_search_button img{
  height: 16px;
}
.navbar-right{
  margin-top: 20px;
}
.navbar-right>li .menu_right {
  position: relative;
  line-height: 20px;  
  display: block;
  padding: 10px 12px;
  color: #433E3C !important; 
}
.no-touch .navbar-right>li:hover{
  background: #EFEFEF !important; 
}
.navbar-right>li .menu_right:focus, 
.navbar-right>li .menu_right:active{
  background: none !important; 
} 
.navbar-right>li .menu_right img{
  height: 32px;
  width: 32px;
  vertical-align: middle;
}
.badge {
  display: block;
  position: absolute;
  top: 2px;
  right: 1px;
  line-height: 20px;
  height: 20px;
  padding: 0 7px;
}
.badge-orange {
  background-color: #ff5600;
}
.badge-toggle {
  display: block;
  position: absolute;
  top: -6px;
  right: -10px;
  line-height: 20px;
  height: 20px;
  padding: 0 7px;
}
.dropdown-menu{
  color: #433E3C;  
  background: #ffffff;    
  box-shadow: none;  
  border-radius: 10px !important;
}
.menu_cart_no_collapse{
  display: block !important; 
}




