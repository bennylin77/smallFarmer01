/*======================================= checkout =======================================*/
.cart-dropdown-menu {
  visibility: visible;
  padding: 20px 20px;  
  width: 315px !important;
  box-shadow: rgba(0, 0, 0, 0.2) 0px 2px 4px 0px;  
}
.cart_lists li{
  border-bottom: 1px solid #E7E7E7;   
  padding-bottom: 10px;  
  margin-bottom: 10px;
}
.cart_lists li a{
  color: #433E3C;  
}
.no-touch .cart_lists li a:hover{
  opacity: 0.8;
}
.cart_image{
  height: 70px; 
  width: 70px;
  display: inline-block;
  vertical-align: middle;
}
.cart_content{
  width: 165px;
  margin-left: 15px;
  display: inline-block; 
  vertical-align: middle;   
}
.cart_op{
  margin-left: 12px;
  display: inline-block; 
  vertical-align: middle;   
}
.cart_label_container{
  padding-bottom: 10px; 
  text-align: right;  
}

@media (max-width: 1065px) {
  .cart-dropdown-menu{
    width: auto !important;
  }
  .cart_lists li{
    text-align: center;
  }
  .cart_lists li a{
    text-align:left;
  }  
  .cart_label_container{
    visibility: hidden;
  } 
}